<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>registerServlet</servlet-name>
    <servlet-class>servlet.control.HandleRegister</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registerServlet</servlet-name>
    <url-pattern>/registerServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>androidlogin</servlet-name>
    <servlet-class>Android.servlet.login_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>androidlogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>picturepath</servlet-name>
    <servlet-class>otherjava.read_picture4</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>picturepath</servlet-name>
    <url-pattern>/picture</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>gouwuche</servlet-name>
    <servlet-class>servlet.control.insert_into</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>gouwuche</servlet-name>
    <url-pattern>/gouwuche</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>media</servlet-name>
    <servlet-class>org.apache.catalina.servlets.DefaultServlet</servlet-class>
    <init-param>
      <param-name>listings</param-name>
      <param-value>true</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>media</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
</web-app>